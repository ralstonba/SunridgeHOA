
@{
    ViewData["Title"] = "AddSummerPic";
}

<h2>Add Picture</h2>

<br />

<form asp-action="../Home/SummerGallery" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="lblAddPicture">Picture</label>
        <input type="file" name="picture" id="picture" />
    </div>
    <br />
    <button type="submit" class="btn btn-primary">Submit</button>
</form>